   "source": [
    "def fact(x):\n",
    "    result = 1\n",
    "    if x==1 or x==0:\n",
    "        print(f\"The {x}! is = 1\")\n",
    "        return 1\n",
    "    else:\n",
    "        for i in range(x):\n",
    "            result = result * (i+1)\n",
    "        return result\n",
    "\n",
    "fact(5)    "
   ]
